{
  "name": "startup-be",
  "version": "1.0.0",
  "description": "A boilerplate Node-Express application to get you started",
  "main": "./dist/index.js",
  "types": "./dist/@types",
  "readme": "./README.md",
  "scripts": {
    "ts-build": "npx tsc",
    "dev:watch": "tsc -w",
    "dev:start-nik": "npm run db:create && npm run ts-build && cross-env NODE_PATH=./dist supervisor ./dist/index.js",
    "dev:start": "npm run db:create && npm run ts-build && cross-env NODE_PATH=./dist nodemon ./dist/index.js",
    "dev": "set NODE_ENV=development ts-node-dev src/index.ts",
    "db:create": "sequelize-cli db:create",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.8",
    "axios": "^1.2.1",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.4.1",
    "node-verification-code": "^1.1.3",
    "nodemailer": "^6.9.13",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.7.8",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "supervisor": "^0.12.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express"
  ],
  "license": "MIT"
}
